# Database
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/academy?schema=public"

# Redis
REDIS_URL="redis://localhost:6379"

# Judge0
JUDGE0_URL="http://localhost:2358"
JUDGE0_AUTH_TOKEN=""
# Set to "true" on macOS to bypass Judge0 (uses mock responses for development)
# Judge0 requires Linux cgroups which aren't available in Docker Desktop
JUDGE0_MOCK_MODE="false"

# App
NEXT_PUBLIC_APP_URL="http://localhost:3000"
NODE_ENV="development"

# Session (generate with: openssl rand -base64 32)
SESSION_SECRET="your-session-secret-here"

# AI Providers (optional - can also configure in admin UI)
ANTHROPIC_API_KEY=sk-ant-...
OPENAI_API_KEY=sk-...
